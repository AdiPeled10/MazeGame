<Window x:Class="GUIClient.SinglePlayerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUIClient"
        mc:Ignorable="d"
        Title="SinglePlayer" Height="200" Width="300">
    <StackPanel Name="stackPanel">
        <local:MazeInformationLayout x:Name="mazeInfo"></local:MazeInformationLayout>
        <Button Content="Start Game" Margin="15" Width="70" Click="Button_Click" ></Button>
    </StackPanel>
</Window>
