<Window x:Class="GUIClient.SinglePlayerMaze"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUIClient"
        mc:Ignorable="d"
        Title="SinglePlayerMaze" Height="600" Width="600">
    <DockPanel Name="dockPanel" Margin="0,0,3.6,1.4">
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
            <Button Content="Restart game" VerticalAlignment="Top" HorizontalAlignment="Left" Height="30" Margin="89,18,0,0"/>
            <Button Content="Solve maze" VerticalAlignment="Top" HorizontalAlignment="Center" Height="30" Margin="68,18,0,0"/>
            <Button Content="Main menu" VerticalAlignment="Top" HorizontalAlignment="Right" Height="30" Margin="93,18,0,0"/>
        </StackPanel>
    </DockPanel>

</Window>
