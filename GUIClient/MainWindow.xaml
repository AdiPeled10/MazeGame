<Window x:Class="GUIClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUIClient"
        mc:Ignorable="d"
        Title="MazeGame" Height="350" Width="525">
    <StackPanel Name="stackPanel">
        <Button Name="singleButton" Content="SinglePlayer" Width="100" HorizontalAlignment="Center"
                VerticalAlignment="Center" Margin="20" Click="SinglePlayerClick" >
            
         
        </Button>
        <Button Content="Multi player" Width="100" HorizontalAlignment="Center"
                VerticalAlignment="Center" Margin ="20" />
        <Button Content= "Settings" Width="100"  >

        </Button>
        <StackPanel.Background>
            <SolidColorBrush Color="Aqua"></SolidColorBrush>
        </StackPanel.Background>

    </StackPanel>
</Window>
